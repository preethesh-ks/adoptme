{
    "extends":[
        "eslint:recommended",
        "plugin:import/errors",
        "plugin:import/recommend",
        "plugin:jsx-a11y/recommend",
        "prettier"
        
    ],
    "rules":{
        "react/prop-types":0,
        "react/react-in-jsx-scope":0
    },
    "plugins": ["react","import","jsx-a11y"],
        "parserOptions": {
            "ecmaVersion":2022,
            "sourceType":"module",
            "ecmaFeatures":{
                "jsx":true
            }
        },
        "env":{
            "es6":true,
            "browser":true,
            "node":true
        },
        "settings": {
            "react":{"version":"detect"}
        },
        "import/resolver":{
            "node":{
                "extensions":[
                    ".js",
                    ".jsx",
                    ".ts",
                    ".tsx"]
            }
        }
    
}
//yarn add eslint-plugin-import@2.26.0 eslint-plugin-jsx-a11y@6.6.1 eslint-plugin-react@7.31.8